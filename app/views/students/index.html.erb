<p id="notice"><%= notice %></p>

<h1>Students</h1>

<table>
  <thead>
    <tr>
      <th>F name</th>
      <th>L name</th>
      <th>Phone</th>
      <th>Address one</th>
      <th>Address two</th>
      <th>Email</th>
      <th>City</th>
      <th>State</th>
      <th>Zip</th>
      <th>Dob</th>
      <th>Course</th>
      <th>Military status</th>
      <th>Education</th>
      <th>Experience</th>
      <th>Employment status</th>
      <th>Salary</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @students.each do |student| %>
      <tr>
        <td><%= student.f_name %></td>
        <td><%= student.l_name %></td>
        <td><%= student.phone %></td>
        <td><%= student.address_one %></td>
        <td><%= student.address_two %></td>
        <td><%= student.email %></td>
        <td><%= student.city %></td>
        <td><%= student.state %></td>
        <td><%= student.zip %></td>
        <td><%= student.dob %></td>
        <td><%= student.course %></td>
        <td><%= student.military_status %></td>
        <td><%= student.education %></td>
        <td><%= student.experience %></td>
        <td><%= student.employment_status %></td>
        <td><%= student.salary %></td>
        <td><%= link_to 'Show', student %></td>
        <td><%= link_to 'Edit', edit_student_path(student) %></td>
        <td><%= link_to 'Destroy', student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Student', new_student_path %>
